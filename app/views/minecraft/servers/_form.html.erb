<%= form_for([:minecraft, @server], html: { class: 'form' }) do |f| %>
  <div class="form-group">
    <%= f.label :host %>
    <%= f.text_field :host, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :jar, 'Minecraft Version' %>
    <%= f.collection_select :jar_id, @jars, :id, :version, {}, class: 'custom-select' %>
  </div>


  <%= f.submit class: 'btn btn-primary' %>
<% end %>
